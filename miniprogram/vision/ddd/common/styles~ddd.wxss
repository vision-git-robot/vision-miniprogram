  padding: 0.35em 0.625em 0.75em;
}

.h5-legend {
  border: 0;
  padding: 0;
}

.h5-textarea {
  overflow: auto;
  resize: vertical;
  vertical-align: top;
}

.h5-optgroup {
  font-weight: bold;
}

.h5-input,
.h5-select,
.h5-textarea {
  outline: 0;
}

.h5-textarea,
.h5-input {
  -webkit-user-modify: read-write-plaintext-only;
}

.h5-input::-ms-clear,
.h5-input::-ms-reveal {
  display: none;
}

.h5-input::-moz-placeholder,
.h5-textarea::-moz-placeholder {
  color: #999;
}

.h5-input:-ms-input-placeholder,
.h5-textarea:-ms-input-placeholder {
  color: #999;
}

.h5-input::-webkit-input-placeholder,
.h5-textarea::-webkit-input-placeholder {
  color: #999;
}

.placeholder {
  color: #999;
}

.h5-table {
  border-collapse: collapse;
  border-spacing: 0;
}

.h5-td,
.h5-th {
  padding: 0;
}

.h5-h1,
.h5-h2,
.h5-h3,
.h5-h4,
.h5-h5,
.h5-h6,
.h5-p,
.h5-figure,
.h5-form,
.h5-blockquote {
  margin: 0;
}

.h5-ul,
.h5-ol,
.h5-li,
.h5-dl,
.h5-dd {
  margin: 0;
  padding: 0;
}

.h5-ul,
.h5-ol {
  list-style: none outside none;
}

.h5-h1,
.h5-h2,
.h5-h3 {
  line-height: 2;
  font-weight: normal;
}

.h5-h1 {
  font-size: 18px;
}

.h5-h2 {
  font-size: 16px;
